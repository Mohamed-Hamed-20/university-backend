{
  "name": "university",
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "builds": {
    "env": {
      "NODE_ENV": "production"
    },
    "scripts": {
      "build": "npm install && npm run build"
    }
  }
}
